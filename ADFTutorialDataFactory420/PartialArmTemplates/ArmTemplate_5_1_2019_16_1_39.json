{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ADFTutorialDataFactory420"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStore_Source')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "https://adftutorial421.azuredatalakestore.net/webhdfs/v1",
					"tenant": "f3fbefe4-0e6e-453c-a762-4f77128e85aa",
					"subscriptionId": "d23af54e-f44c-4acb-92fe-59863bb601e4",
					"resourceGroupName": "ADFTutorialResourceGroup"
				}
			}
		}
	]
}