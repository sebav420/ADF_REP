{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ADFTutorialDataFactory420"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"AzStorage_Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzStorage_Staging'"
		},
		"AzureDataLakeStore_Source_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://adftutorial421.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore_Source_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "f3fbefe4-0e6e-453c-a762-4f77128e85aa"
		},
		"AzureDataLakeStore_Source_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "d23af54e-f44c-4acb-92fe-59863bb601e4"
		},
		"AzureDataLakeStore_Source_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "ADFTutorialResourceGroup"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"AzStorage_Staging_connectionString": {
						"value": "[parameters('AzStorage_Staging_connectionString')]"
					},
					"AzureDataLakeStore_Source_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore_Source_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore_Source_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore_Source_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore_Source_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore_Source_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore_Source_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore_Source_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}